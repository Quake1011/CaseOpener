
"UnlitGeneric"
{
   $basetexture "particle\electrical\electrical_arc_sprites"
   "$vertexcolor" 		1 
   "$vertexalpha" 		1 
   "$nolod" 			1
   "$translucent" 1
	$additive 1

   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "30"
      }
   }
}